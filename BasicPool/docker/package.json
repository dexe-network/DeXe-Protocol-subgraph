{
  "name": "BasicPool",
  "license": "MIT",
  "scripts": {
    "compile": "graph codegen && graph build",
    "create": "graph create --node $GRAPH_NODE_ENDPOINT BasicPool",
    "remove": "graph remove --node $GRAPH_NODE_ENDPOINT BasicPool",
    "deploy-only": "graph deploy --node $GRAPH_NODE_ENDPOINT --ipfs $IPFS_ENDPOINT --version-label $GRAPH_VERSION BasicPool",
    "compile-and-deploy": "npm run compile && npm run deploy-only",
    "update-config": "node scripts/config-parser.js"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.32.0",
    "@graphprotocol/graph-ts": "^0.27.0",
    "dotenv": "^16.0.3"
  }
}
